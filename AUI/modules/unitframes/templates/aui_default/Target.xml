          <Anchor point="TOPLEFT" />
          <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM" />
          <Controls>
            <StatusBar name="$(parent)Gloss" tier="MEDIUM" textureFile="AUI/images/attributes/aui/target/bar_gloss.dds" alpha="0.2" level="450" >
              <LeadingEdge textureFile="AUI/images/attributes/aui/target/bar_leading_edge_gloss.dds" width="64" height="64" />
              <AnchorFill/>
            </StatusBar>
          </Controls>
        </StatusBar>
        <StatusBar name="$(parent)_Bar2" textureFile="AUI/images/attributes/aui/target/bar.dds" tier="MEDIUM" level="400">
          <LeadingEdge textureFile="AUI/images/attributes/aui/target/bar_leading_edge.dds" width="64" height="64" />
          <Anchor point="TOPLEFT" relativePoint="TOP" />
          <Anchor point="BOTTOMRIGHT" />
          <Controls>
            <StatusBar name="$(parent)Gloss" tier="MEDIUM" textureFile="AUI/images/attributes/aui/target/bar_gloss.dds" alpha="0.2" level="450" >
              <LeadingEdge textureFile="AUI/images/attributes/aui/target/bar_leading_edge_gloss.dds" width="64" height="64" />
              <AnchorFill/>
            </StatusBar>
          </Controls>
        </StatusBar>
      </Controls>
    </Control>
  </Controls>
</GuiXml>
